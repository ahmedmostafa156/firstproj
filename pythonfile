while True :
    
     def fn(a,b,c):
         
         if a>b and a>c:
             
             return(a)
         elif b>a and b>c:
             return(b)
    
         else:
             return(c)
    
     a=int(raw_input("enter your first num : "))
     b=int(raw_input("enter your second num : "))
     c=int(raw_input("enter your third num : "))

     print(fn(a,b,c))


